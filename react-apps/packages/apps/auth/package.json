{
  "name": "@elgatoylacaja/auth",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "build": "react-scripts build",
    "build:production": "env-cmd -f .env npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "npm run format",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@elgatoylacaja/components": "0.1.0",
    "@elgatoylacaja/hooks": "0.1.0",
    "@elgatoylacaja/utils": "0.1.0",
    "env-cmd": "^10.1.0",
    "formik": "^2.1.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "typescript": "^3.8.2"
  },
  "devDependencies": {
    "@types/facebook-js-sdk": "^3.2.1",
    "@types/gapi": "0.0.36",
    "@types/gapi.auth2": "0.0.50",
    "@types/node": "^13.7.6",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "react-scripts": "^3.4.0"
  }
}
